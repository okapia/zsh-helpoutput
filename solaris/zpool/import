usage:
	import [-d path ... ] [-D]
	import [-d path ... | -c cachefile] [-F [-n]] <pool | id>
	import [-o mntopts] [-o property=value] ... 
	    [-d path ... | -c cachefile] [-D] [-f] [-m] [-N] [-R root] [-F [-n]] -a
	import [-o mntopts] [-o property=value] ... 
	    [-d path ... | -c cachefile] [-D] [-f] [-m] [-N] [-R root] [-F [-n]]
	    <pool | id> [newpool]
