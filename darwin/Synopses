# macOS specific commands

cmd chflags
version 10.11.6
sw_vers -productVersion | getversion
emit -? | getusage

cmd defaults
version 10.11.6
sw_vers -productVersion | getversion
emit help | getusage

cmd hdiutil
version 10.11.6
subdir
sw_vers -productVersion | getversion
emit help | getusage
subcmds @ -help

cmd open
version 10.11.6
sw_vers -productVersion | getversion
emit --help | getusage

cmd softwareupdate
version 10.11.6
sw_vers -productVersion | getversion
emit -h | getusage

cmd system_profiler
version 10.11.6
sw_vers -productVersion | getversion
emit -h | getusage

