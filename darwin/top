top usage: top [-a | -d | -e | -c <mode>]
               [-F | -f]
               [-h]
               [-k]
               [-L | -l <samples>]
               [-o <key>] [-O <skey>]
               [-R | -r]
               [-s <delay>]
               [-T | -t]
               [-U <user>]
               [-u]
               [-W | -w]
               [-X | x]
               [[-n] <nprocs>]
